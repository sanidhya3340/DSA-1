#include<stdio.h>
int main()
{
    int n;int d;int i;int j;
    int a[100];int k=0;int t;int temp;
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {d=1;int e=0;t=d+e;
      for(j=0;j<=i;j++)
      {
          a[k]=d;
          t=d+e;
          e=d;
          d=t;
          k++;
      }
    }for(i=0;i<k;i++){
    printf("%d ",a[i]);
}printf("\n");
for(i=0;i<k/2;i++)
{
 temp=a[k-1-i];
 a[k-1-i]=a[i];
a[i]=temp;
}
for(i=0;i<k;i++){
    printf("%d ",a[i]);
}
}
